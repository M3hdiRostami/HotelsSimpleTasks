{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ScreenshotMarkerConfig": {
    "Markers": [
      {
        "selector": ".hp__hotel-name",
        "color": "red"
      },
      {
        "selector": ".hp__hotel_ratings",
        "color": "red"
      },
